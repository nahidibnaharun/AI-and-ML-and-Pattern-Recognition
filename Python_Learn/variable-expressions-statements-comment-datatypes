#variables
name="abc"
#type function to check the type of variable
print(type(name))
print(type(name)==str)
#isinstance function to check the type of variable
print(isinstance(name,str))
age=float(2)
print(isinstance(age,float))
#type conversion to string to int
number="23"
cn=int(number)
print(isinstance(cn,int))




 
